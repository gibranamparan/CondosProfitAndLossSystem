<body>
   <!-- START Main wrapper-->
   <section class="wrapper" id="SectionP">
      <!-- START Top Navbar-->
      <nav id="ParteLogo" role="navigation" class="navbar navbar-default navbar-top navbar-fixed-top">
         <!-- START navbar header-->
         <div   class="navbar-header">
            <a href="#" class="navbar-brand" id="logoss">

                <dv class="brand-logo"><img src="~/Imagenes/systemLogo.png" width="169"></dv>
            </a>
         </div>
          
         <!-- END navbar header-->
         <!-- START Nav wrapper-->
         <div class="nav-wrapper">
            <!-- START Left navbar-->
            <ul class="nav navbar-nav">
               <li>
                  <a href="#" data-toggle="aside">
                     <em class="fa fa-align-left"></em>
                  </a>
               </li>
               <li>
                  @*<a href="#" data-toggle="navbar-search">
                   <em class="fa fa-search"></em>
                </a>*@
               </li>
            </ul>
            <!-- END Left navbar-->
            <!-- START Right Navbar-->
             <ul class="nav navbar-nav navbar-right" style="padding-right: 1em">
                 <li class="pull-right">

                     @if (User.Identity.IsAuthenticated)
                     {
                         if (User.IsInRole("Administrador"))
                         {
                             <div id="TextoLog">
                                 Hello<strong> @NombreCompleto</strong>,<br /> you are logged as <br /><strong>Administrator.</strong>

                             </div>
                         }
                         else
                         {
                             <div id="TextoLog">
                                 Hello <strong>@NombreCompleto</strong>,<br /> you are logged as <br /> <strong>Client.</strong>

                             </div>

                         }
                     }
                 </li>
             </ul>
         </div>
      </nav>
      <!-- END Top Navbar-->
      <!-- START aside-->
      <aside id="BarraSide" class="aside hidden-print">
         <!-- START Sidebar (left)-->
         <nav class="sidebar">
            <ul class="nav">
               <!-- START user info-->
               <li>
                  <!-- START User links collapse-->
                  <ul class="nav collapse">
                     <li><a href="#">Profile</a>
                     </li>
                     <li><a href="#">Settings</a>
                     </li>
                     <li><a href="#">Notifications<div class="label label-danger pull-right">120</div></a>
                     </li>
                     <li><a href="#">Messages<div class="label label-success pull-right">300</div></a>
                     </li>
                     <li class="divider"></li>
                     <li><a href="#">Logout</a>
                     </li>
                  </ul>
                  <!-- END User links collapse-->
               </li>
               @if (User.IsInRole("Administrador"))
               {
               <li>
                  <a href="@Url.Action("Index", "Account")" title="Owners" >
                     <em class="fa fa-users"></em>
                     <span class="item-text">Owners</span>
                  </a>
                 
               </li>
               <li>
                  <a href="@Url.Action("Index", "Houses")"  title="Houses">
                     <em class="fa fa-home"></em>
                     <span class="item-text">Houses</span>
                  </a>
               </li>
                <li>
                    <a href="@Url.Action("Index", "Service")" title="Houses">
                        <em class="fa fa-sticky-note"></em>
                        <span class="item-text">Report Settings</span>
                    </a>
                </li>
              
               }
               else if (User.IsInRole("Cliente"))
               {
                <li>
                    <a href="@Url.Action("Index","Houses")" title="Houses">
                        <em class="fa fa-home"></em>
                        <span class="item-text">Reports</span>
                    </a>
                </li>
                <li>
                    @using Microsoft.AspNet.Identity
                    <a href="@Url.Action("Details", "Account", new { id = User.Identity.GetUserId() }, null)" title="Houses">
                        <em class="fa fa-user"></em>
                        <span class="item-text">My Account</span>
                    </a>
               
                </li>
               }

                @if (User.Identity.IsAuthenticated)
                {
                <li>
                    <a href="@Url.Action("LogOff","Account")" title="Log Off">
                        <em class="fa fa-sign-out"></em>
                        <span class="item-text">Log Off</span>
                    </a>
                </li>
                }
                <li>
                    <a href="http://sunvalleyservicesrockypoint.com/" target="_blank" title="Web Site">
                        <em class="fa fa-internet-explorer"></em>
                        <span class="item-text">Web Site</span>
                    </a>
                </li>
               <!-- END Menu-->
               <!-- Sidebar footer    -->
               <li class="nav-footer">
                  <div class="nav-footer-divider"></div>
               </li>
            </ul>
         </nav>
         <!-- END Sidebar (left)-->
      </aside>
      <!-- START Main section-->
      <section>
         <!-- START Page content-->
         <section class="main-content">
             @RenderBody()
         </section>          <!-- END Page content-->          <!-- END Main section-->
